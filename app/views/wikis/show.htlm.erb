<div class="container-fluid">
    <h1>  <%= @wiki.title %> </h1>
    <p><%= @wiki.body %></p>
    <hr />
    <div>
      <%= link_to "Edit", edit_wiki_path(@wiki), class:'btn btn-primary' %>
      <%= link_to "Delete", [@wiki], method: :delete, class:'btn btn-danger', data: {confirm: 'Are you sure you want to delete this wiki?'} %>
    </div>
</div>
